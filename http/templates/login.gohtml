{{template "inc/header"}}

<h1>Login</h1>

<form action="/login" method="POST">
    <div>
        <label for="">Email</label>
        <input type="email" name="email">
    </div>
    <div>
        <label for="">Password</label>
        <input type="password" name="password">
    </div>
    <div>
        <input type="submit">
    </div>
</form>

<b>{{.}}</b>

{{template "inc/footer"}}
